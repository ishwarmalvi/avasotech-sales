<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="sequence_crmlead" model="ir.sequence">
            <field name="name">CRM Lead Code</field>
            <field name="code">crm.lead</field>
            <field name="padding">3</field>
            <field name="prefix">OPT</field>
        </record>

        <record id="seq_crm_to_saleorder" model="ir.sequence">
            <field name="name">Sales Order</field>
            <field name="code">crm.sale.order</field>
            <field name="prefix">QTE</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
